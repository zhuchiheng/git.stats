{
  "name": "git-stats-plus",
  "displayName": "Git Stats Plus",
  "description": "Beautiful visualization of team code contributions with interactive charts and insights, base on project git-stats of @lixianmin.",
  "version": "0.1.4",
  "publisher": "zhuchiheng",
  "author": {
    "name": "zhuchiheng"
  },
  "license": "MIT",
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "git",
    "statistics",
    "visualization",
    "team",
    "contribution"
  ],
  "galleryBanner": {
    "color": "#6C5CE7",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhuchiheng/git.stats"
  },
  "bugs": {
    "url": "https://github.com/zhuchiheng/git.stats/issues"
  },
  "homepage": "https://github.com/zhuchiheng/git.stats#readme",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-stats.showStats",
        "title": "Show Git Stats",
        "icon": "$(graph-line)"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "when": "scmProvider == git",
          "command": "git-stats.showStats",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "simple-git": "^3.20.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/d3-array": "^3.2.1",
    "@types/d3-axis": "^3.0.6",
    "@types/d3-brush": "^3.0.6",
    "@types/d3-chord": "^3.0.6",
    "@types/d3-color": "^3.1.3",
    "@types/d3-contour": "^3.0.6",
    "@types/d3-delaunay": "^6.0.4",
    "@types/d3-dispatch": "^3.0.6",
    "@types/d3-drag": "^3.0.7",
    "@types/d3-dsv": "^3.0.7",
    "@types/d3-ease": "^3.0.2",
    "@types/d3-fetch": "^3.0.7",
    "@types/d3-force": "^3.0.10",
    "@types/d3-format": "^3.0.4",
    "@types/d3-geo": "^3.1.0",
    "@types/d3-hierarchy": "^3.1.7",
    "@types/d3-interpolate": "^3.0.4",
    "@types/d3-path": "^3.1.1",
    "@types/d3-polygon": "^3.0.2",
    "@types/d3-quadtree": "^3.0.6",
    "@types/d3-random": "^3.0.3",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-scale-chromatic": "^3.1.0",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-time": "^3.0.4",
    "@types/d3-time-format": "^4.0.3",
    "@types/d3-timer": "^3.0.2",
    "@types/d3-transition": "^3.0.9",
    "@types/d3-zoom": "^3.0.8",
    "@types/geojson": "^7946.0.16",
    "@types/jquery": "^3.5.32",
    "@types/node": "^16.11.7",
    "@types/sizzle": "^2.3.9",
    "@types/vscode": "^1.60.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.2.2",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  }
}
